{
  "name": "gyscontrol-app",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/artemiogeek/gyscontrol-app.git"
  },
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --selectProjects='Unit Tests'",
    "test:integration": "jest --selectProjects='Integration Tests'",
    "test:accessibility": "jest --selectProjects='Accessibility Tests'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report",
    "test:e2e:accessibility": "playwright test --project=accessibility",
    "test:e2e:performance": "playwright test --project=performance",
    "test:coverage:json": "jest --coverage --coverageReporters=json",
    "test:coverage:threshold": "jest --coverage --coverageThreshold='{}'",
    "test:report": "jest --coverage --coverageReporters=html",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:accessibility && npm run test:e2e",
    "test:aprovisionamiento": "jest src/__tests__/**/*aprovisionamiento*",
    "test:aprovisionamiento:unit": "jest src/__tests__/services/aprovisionamiento.test.ts src/__tests__/components/aprovisionamiento.test.tsx src/__tests__/api/aprovisionamiento.test.ts",
    "test:aprovisionamiento:integration": "jest src/__tests__/integration/aprovisionamiento.integration.test.tsx",
    "test:aprovisionamiento:accessibility": "jest src/__tests__/accessibility/aprovisionamiento.accessibility.test.tsx",
    "test:aprovisionamiento:e2e": "playwright test src/__tests__/e2e/aprovisionamiento.e2e.test.ts",
    "test:aprovisionamiento:all": "npm run test:aprovisionamiento:unit && npm run test:aprovisionamiento:integration && npm run test:aprovisionamiento:accessibility && npm run test:aprovisionamiento:e2e",
    "test:cotizaciones": "jest src/__tests__/**/*cotizacion*",
    "test:cotizaciones:unit": "jest src/__tests__/lib/utils/cotizacionCodeGenerator.test.ts src/__tests__/components/cotizaciones/CrearCotizacionModal.test.tsx src/__tests__/app/api/cotizacion/from-plantilla/route.test.ts",
    "test:cotizaciones:coverage": "jest src/__tests__/**/*cotizacion* --coverage",
    "test:client": "jest --testEnvironment=jsdom src/__tests__/components/**/*.test.tsx src/__tests__/hooks/**/*.test.ts",
    "test:server": "jest --testEnvironment=node src/__tests__/lib/**/*.test.ts src/__tests__/app/api/**/*.test.ts",
    "test:ci": "npm run test:client && npm run test:server",
    "analyze": "cross-env ANALYZE=true next build",
    "type-check": "tsc --noEmit",
    "format:check": "prettier --check .",
    "audit:consistency": "tsx scripts/audit-consistency.ts",
    "audit:models": "tsx scripts/audit-consistency.ts --phase=models",
    "audit:types": "tsx scripts/audit-consistency.ts --phase=types",
    "audit:payloads": "tsx scripts/audit-consistency.ts --phase=payloads",
    "audit:validators": "tsx scripts/audit-consistency.ts --phase=validators",
    "audit:apis": "tsx scripts/audit-consistency.ts --phase=apis",
    "audit:services": "tsx scripts/audit-consistency.ts --phase=services",
    "audit:components": "tsx scripts/audit-consistency.ts --phase=components",
    "audit:pages": "tsx scripts/audit-consistency.ts --phase=pages",
    "audit:sidebar": "tsx scripts/audit-consistency.ts --phase=sidebar",
    "audit:testing": "tsx scripts/audit-consistency.ts --phase=testing",
    "audit:entity": "tsx scripts/audit-consistency.ts --entity=",
    "audit:phase:entity": "tsx scripts/audit-consistency.ts --phase= --entity=",
    "generate:entity": "tsx scripts/generate-entity.ts",
    "generate:types": "tsx scripts/generate-types.ts",
    "db:audit": "npm run audit:consistency && npm run type-check",
    "db:backup": "tsx scripts/extract-current-data.ts",
    "db:restore": "tsx scripts/restore-current-data.ts",
    "postinstall": "prisma generate",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.0.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-accordion": "^1.2.4",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.1",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.8",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@react-pdf/renderer": "^4.3.0",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-query-devtools": "^5.87.4",
    "@types/lodash": "^4.17.20",
    "@types/react-window": "^1.8.8",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "lodash": "^4.17.21",
    "lucide-react": "^0.485.0",
    "next": "15.2.4",
    "next-auth": "^4.24.11",
    "node-fetch": "^3.3.2",
    "pdf-parse": "^1.1.1",
    "react": "^19.0.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.1",
    "react-hot-toast": "^2.5.2",
    "react-window": "^2.1.0",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^3.2.0",
    "sonner": "^2.0.3",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.2.0",
    "tw-animate-css": "^1.2.5",
    "xlsx": "^0.18.5",
    "zod": "^3.24.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-attributes": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3",
    "@jest/globals": "^30.1.2",
    "@playwright/test": "^1.55.0",
    "@swc/jest": "^0.2.39",
    "@tailwindcss/postcss": "^4.1.12",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^5.0.2",
    "@types/date-fns": "^2.5.3",
    "@types/jest": "^30.0.0",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "babel-jest": "^30.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.3",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.1.2",
    "jest-mock-extended": "^4.0.0",
    "jsdom": "^25.0.1",
    "msw": "^2.11.1",
    "playwright": "^1.55.0",
    "prisma": "^6.16.2",
    "puppeteer": "^24.20.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.4",
    "typescript": "^5",
    "vitest": "^2.1.8"
  }
}
